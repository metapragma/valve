{
  "name": "valve",
  "description": "minimal pull stream",
  "version": "2.0.1",
  "homepage": "https://github.com/escapace/ts-pull-stream",
  "repository": {
    "type": "git",
    "url": "git://github.com/escapace/ts-pull-stream.git"
  },
  "types": "./types/index.d.ts",
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "side-effects": false,
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "build:dts": "rm -rf lib types && tsc && rm -rf lib",
    "build:cjs": "cross-env NODE_ROLLUP_TARGET=cjs rollup -c -o lib/cjs/index.js",
    "build:es": "cross-env NODE_ROLLUP_TARGET=es rollup -c -o lib/es/index.js",
    "build": "npm run build:dts && npm run build:cjs && npm run build:es",
    "test:karma": "karma start --single-run",
    "test:node": "ts-node -O '{ \"declaration\": false, \"module\": \"CommonJS\" }' node_modules/.bin/tape test/*.js test/*.ts",
    "test": "npm run test:node && npm run test:karma",
    "test:travis": "npm run test:node && npm run test:karma"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.85",
    "@types/tape": "^4.2.31",
    "cross-env": "^5.1.1",
    "fastbench": "^1.0.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-tap": "^3.1.1",
    "karma-typescript": "^3.0.8",
    "karma-typescript-es6-transform": "^1.0.2",
    "puppeteer": "^0.13.0",
    "rollup": "^0.51.8",
    "rollup-plugin-typescript": "^0.8.1",
    "setimmediate": "^1.0.5",
    "tape": "~4.8.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.6.1"
  },
  "author": "escapace",
  "license": "MPL-2.0",
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
